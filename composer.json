{
    "name": "soneritics/framework",
    "type": "library",
    "description": "Soneritics PHP framework for PHP 5.5 and higher.",
    "homepage": "http://framework.soneritics.nl/",
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "pear/pear_exception": "dev-master",
        "pear/mail_mime": "dev-master",
        "pear/structures_graph": "dev-master",
        "pear/console_getopt": "dev-master",
        "pear/pear": "dev-master",
        "pear/mail": "dev-master",
        "firephp/firephp-core": "*",
        "psr/log": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "dev-master"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear2.php.net"
        },
        {
            "type": "vcs",
            "url": "https://github.com/firephp/firephp-core"
        }
